ZPPartage.prototype.init=function(){
this.POST_CONTENT_TYPE={
"Content-Type":"application/x-www-form-urlencoded;charset=UTF-8"};
this.tabApps=[];
this.defaultTabAppNames=[];
this.customTabAppNames=[];
this.renaterArray=[];
this.domainArray=[];
this.currentTabAppName=null;
