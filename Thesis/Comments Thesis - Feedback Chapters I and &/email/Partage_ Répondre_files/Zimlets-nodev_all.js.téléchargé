var l=new AjxCallback(this,this._handleResponseGetContact,[n,h,o,t,d]);
r.getContactByEmail(o.address,l)
}
return this._updateHtmlContent(n,h,o,s,t,d)
}else{
n[h++]=AjxStringUtil.htmlEncode(o);
return h
}};
