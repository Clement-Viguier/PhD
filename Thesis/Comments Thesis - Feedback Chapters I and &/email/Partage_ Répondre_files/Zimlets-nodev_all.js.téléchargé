Com_Zimbra_Date.validate=function(e,n,t){
if(e<1||e>31){
return false
}
if(n<0||n>11){
return false
}
if((n==3||n==5||n==8||n==10)&&e==31){
return false
}
if(t&&n==1){
var o=(t%4==0&&(t%100!=0||t%400==0));
if(e>29||(e==29&&!o)){
return false
}}
return true
};
