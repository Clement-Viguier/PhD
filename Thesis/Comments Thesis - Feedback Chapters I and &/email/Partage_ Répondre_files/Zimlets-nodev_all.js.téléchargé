r.setContent('<div id="zimletTooltipDiv"/>',true);
this.x=e;
this.y=h;
if(!this.toolTipPoppedUp(s,t,n,document.getElementById("zimletTooltipDiv"))){
r.popup(e,h,true,new AjxCallback(this,this.hoverOut,t,n,s))
}
return true
};
