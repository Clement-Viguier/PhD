var t=e?e.getContactByEmail(this.emailZimlet.emailAddress):null;
if(!appCtxt.get(ZmSetting.GAL_ENABLED)){
this._handleContactDetails(t)
}else{
var r,n,o;
r={
SearchGalRequest:{
_jsns:"urn:zimbraAccount"}};
n=r.SearchGalRequest;
n.type="account";
